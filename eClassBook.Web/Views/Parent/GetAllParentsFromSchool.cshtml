@model IEnumerable<eClassBook.ViewModels.Parent.ParentViewModel>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<h1>Get all parents from school</h1>



@foreach (var item in Model)
{
    <div>
        <p>First Name: @item.FullName/p>
        <p>Second Name: @item.Address</p>
        <p>Class Grade: @item.Email</p>
        <hr/>
            @foreach(var children in item.Children)
            {
                <p>@children</p>
            }

    </div>
}

<hr />
