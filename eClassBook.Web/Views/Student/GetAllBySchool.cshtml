@model IEnumerable<eClassBook.ViewModels.Student.StudentTableViewModel>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<h1>Get All by School</h1>


@foreach(var item in @Model)
{
    <div>
        <p>@item.SchoolUserId</p>
        <p>@item.FullName</p>
        <p>@item.Grade</p>
        <p>@item.Address</p>
    </div>
    <hr />
}
